{
        "version": "0.2.0",
        "configurations": [
            {
                "name": "(gdb) Launch",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceRoot}/build/learn_it/slam_app",
                // "program": "${workspaceRoot}/build/learn_it/evaluation",
                "args": ["GLOG_v=2"],
                "stopAtEntry": false,
                "cwd": "${workspaceRoot}",
                "environment": [],
                "externalConsole": true,
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ],
                "visualizerFile": "${workspaceRoot}/.vscode/my.natvis",
                "showDisplayString": true
            }
        ]
    }